
public abstract class Output extends IO
{
	private static Output output; 
	
	private Output()
	{
		
	}
	
	public static Output GetInstance()
	{
		if (output == null)
		{
			output = new Output(); 
		}
		else 
			return output; 
	}
	
	
	
	public void Write(String message)
	{
		System.out.print(dateTimeString + ": " + message);
	}

}
